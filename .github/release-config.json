{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## ğŸ› Bug Fixes",
      "labels": ["bug", "fix"]
    },
    {
      "title": "## ğŸ“š Documentation",
      "labels": ["documentation", "docs"]
    },
    {
      "title": "## ğŸ”§ Maintenance",
      "labels": ["maintenance", "chore"]
    },
    {
      "title": "## ğŸ”’ Security",
      "labels": ["security"]
    }
  ],
  "ignore_labels": [
    "ignore",
    "wontfix",
    "duplicate"
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n**Full Changelog**: ${{RELEASE_DIFF}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "- No changes",
  "label_extractor": [
    {
      "pattern": "^(feat|feature)",
      "target": "feature"
    },
    {
      "pattern": "^(fix|bugfix)",
      "target": "bug"
    },
    {
      "pattern": "^docs?",
      "target": "documentation"
    },
    {
      "pattern": "^(chore|maintenance)",
      "target": "maintenance"
    }
  ]
}
